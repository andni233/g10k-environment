mod 'git_module_with_ssh_agent',
  :git => 'git@local.git.server:foo/puppetlabs-firewall.git',
  :use_ssh_agent => true

mod 'git_module_with_ssh_add',
  :git => 'git@local.git.server:bar/puppetlabs-firewall.git'

mod 'github_module_without_ssh_add',
  :git => 'git@github.com:puppetlabs/puppetlabs-firewall.git'

